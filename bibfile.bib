@article{rajalaVILLELanguageIndependentProgram2007,
  title = {{{VILLE}} \textendash{} {{A Language-Independent Program Visualization Tool}}},
  author = {Rajala, Teemu and Laakso, Mikko-Jussi and Kaila, Erkki and Salakoski, Tapio},
